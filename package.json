{
  "name": "prismatakehomechallenge",
  "version": "1.0.0",
  "description": "Prisma ORM Take Home Assignment",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "repl": "npm run build && node dist/index.js",
    "ts-check": "tsc --noEmit",
    "lint": "eslint src",
    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "author": "Artem Kolichenkov",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  }
}
